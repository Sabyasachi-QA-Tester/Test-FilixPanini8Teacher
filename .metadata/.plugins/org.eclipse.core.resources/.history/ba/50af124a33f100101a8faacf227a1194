package pageObjects;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class ChaptersPage extends BasePage {

	public ChaptersPage(WebDriver driver) {
		super(driver);
	}
	
	
	@FindBy(xpath="//input[@placeholder='Search chapters by name...']")
	WebElement txt_SearchChapters;
	
	@FindBy(xpath="//a[@href='/school/chapter/create']")
	WebElement btn_AddNewChapter;
	
	
	
	

}
